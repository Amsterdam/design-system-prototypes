    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.37.0",
    "clsx": "2.1.1",
    "next": "15.5.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "7.64.0",
    "sharp": "0.34.4"
