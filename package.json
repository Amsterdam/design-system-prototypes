{
  "version": "0.0.0",
  "author": "City of Amsterdam",
  "description": "Prototyping environment for the Amsterdam Design System",
  "name": "design-system-prototypes",
  "private": true,
  "engines": {
    "node": "20",
    "npm": "10"
  },
  "type": "module",
  "scripts": {
    "add": "plop",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "link": "npm link @amsterdam/design-system-assets @amsterdam/design-system-css @amsterdam/design-system-react @amsterdam/design-system-react-icons @amsterdam/design-system-tokens",
    "unlink": "npm unlink @amsterdam/design-system-assets @amsterdam/design-system-css @amsterdam/design-system-react @amsterdam/design-system-react-icons @amsterdam/design-system-tokens",
    "update": "npm-check-updates --configFileName .ncurc.major.js --deep --dep dev,prod --target latest --upgrade && npm install",
    "update:minor": "npm-check-updates --configFileName .ncurc.minor.js --deep --dep dev,prod --target minor --upgrade && npm install",
    "update:patch": "npm-check-updates --configFileName .ncurc.patch.js --deep --dep dev,prod --target patch --upgrade && npm install"
  },
  "dependencies": {
    "@amsterdam/design-system-assets": "0.3.0",
    "@amsterdam/design-system-css": "0.16.0",
    "@amsterdam/design-system-react": "0.16.0",
    "@amsterdam/design-system-react-icons": "0.1.14",
    "@amsterdam/design-system-tokens": "0.16.0",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.27.0",
    "clsx": "2.1.1",
    "next": "15.3.2",
    "react": "19.1.0",
    "react-dom": "18.3.1",
    "react-hook-form": "7.56.4",
    "sharp": "0.34.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "15.3.2",
    "@types/react": "19.1.5",
    "@types/react-dom": "18.3.7",
    "eslint": "9.27.0",
    "eslint-config-next": "15.3.2",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.4.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "npm-check-updates": "18.0.1",
    "plop": "4.0.1",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.32.1"
  },
  "overrides": {
    "eslint": "9.27.0",
    "node-plop": {
      "inquirer": "9.3.5"
    }
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ]
}
