{
  "version": "0.0.0",
  "author": "City of Amsterdam",
  "description": "Prototyping environment for the Amsterdam Design System",
  "name": "design-system-prototypes",
  "private": true,
  "engines": {
    "node": "24",
    "npm": "11"
  },
  "type": "module",
  "scripts": {
    "add": "plop",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "link": "npm link @amsterdam/design-system-assets @amsterdam/design-system-css @amsterdam/design-system-react @amsterdam/design-system-react-icons @amsterdam/design-system-tokens",
    "unlink": "npm unlink @amsterdam/design-system-assets @amsterdam/design-system-css @amsterdam/design-system-react @amsterdam/design-system-react-icons @amsterdam/design-system-tokens",
    "update": "npm-check-updates --upgrade && npm install",
    "update:minor": "npm-check-updates --target minor --upgrade && npm install",
    "update:patch": "npm-check-updates --target patch --upgrade && npm install"
  },
  "dependencies": {
    "@amsterdam/design-system-assets": "2.0.0",
    "@amsterdam/design-system-css": "3.1.0",
    "@amsterdam/design-system-react": "3.1.0",
    "@amsterdam/design-system-react-icons": "2.0.0",
    "@amsterdam/design-system-tokens": "3.1.0",
    "clsx": "2.1.1",
    "next": "16.1.6",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-hook-form": "7.71.1",
    "sharp": "0.34.5"
  },
  "devDependencies": {
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/parser": "8.54.0",
    "eslint": "10.0.0",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-perfectionist": "5.4.0",
    "eslint-plugin-prettier": "5.5.5",
    "npm-check-updates": "19.3.2",
    "plop": "4.0.5",
    "prettier": "3.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0"
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
