{
  "version": "0.0.0",
  "author": "City of Amsterdam",
  "description": "Prototyping environment for the Amsterdam Design System",
  "name": "design-system-prototypes",
  "private": true,
  "engines": {
    "node": "20",
    "npm": "10"
  },
  "type": "module",
  "scripts": {
    "add": "plop",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "link": "npm link @amsterdam/design-system-assets @amsterdam/design-system-css @amsterdam/design-system-react @amsterdam/design-system-react-icons @amsterdam/design-system-tokens",
    "unlink": "npm unlink @amsterdam/design-system-assets @amsterdam/design-system-css @amsterdam/design-system-react @amsterdam/design-system-react-icons @amsterdam/design-system-tokens",
    "update": "npm-check-updates --configFileName .ncurc.major.js --deep --dep dev,prod --target latest --upgrade && npm install",
    "update:minor": "npm-check-updates --configFileName .ncurc.minor.js --deep --dep dev,prod --target minor --upgrade && npm install",
    "update:patch": "npm-check-updates --configFileName .ncurc.patch.js --deep --dep dev,prod --target patch --upgrade && npm install"
  },
  "dependencies": {
    "@amsterdam/design-system-assets": "0.3.0",
    "@amsterdam/design-system-css": "0.14.1",
    "@amsterdam/design-system-react": "0.14.1",
    "@amsterdam/design-system-react-icons": "0.1.13",
    "@amsterdam/design-system-tokens": "0.14.0",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.23.0",
    "clsx": "2.1.1",
    "next": "15.2.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.56.0",
    "sharp": "0.33.5"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "15.2.4",
    "@types/eslint__js": "8.42.3",
    "@types/react": "18.3.20",
    "@types/react-dom": "18.3.6",
    "eslint": "9.23.0",
    "eslint-config-next": "15.2.4",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "npm-check-updates": "17.1.16",
    "plop": "4.0.1",
    "prettier": "3.5.3",
    "typescript": "5.8.2",
    "typescript-eslint": "8.29.0"
  },
  "overrides": {
    "eslint": "9.23.0",
    "node-plop": {
      "inquirer": "9.3.5"
    }
  }
}
